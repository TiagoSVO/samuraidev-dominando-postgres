SELECT * FROM LANCAMENTO;
SELECT * FROM CONTA;


SELECT CO.NOME AS "BANCO",
	   SUM(LA.VALOR) AS "VALOR"
FROM CONTA CO
INNER JOIN LANCAMENTO LA
ON CO.ID = LA.ID_CONTA
WHERE LA.SITUACAO='PAGO'
GROUP BY CO.NOME
UNION
SELECT 'TOTAL GERAL' AS "BANCO", 
		SUM(LA.VALOR) AS "VALOR"
FROM LANCAMENTO LA
WHERE LA.SITUACAO='PAGO'
GROUP BY "BANCO";
